    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^16.13.1",
    "react-social-login-buttons": "^3.1.0",
    "router": "^1.3.5"
  },
